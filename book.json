{
  "title": "Sunbreak Personalization API",
  "description": "Sunbreak Personalization API",
  "gitbook": "3.x.x",
  "styles": {
    "website": "styles/website.css"
  },
  "structure": {
    "readme": "README.md",
    "glossary": "GLOSSARY.md"
  },
  "variables": {
    "apiUrl": "https://api.sunbreak.dev/"
  },
  "plugins": [
    "theme-api",
    "collapsible-menu",
    "hide-published-with",
    "include-codeblock",
    "ace",
    "term",
    "copy-code-button",
    "insert-logo"
  ],
  "pluginsConfig": {
    "theme-api": {
      "theme": "dark",
      "split": true
    },
    "include-codeblock": {
      "template": "ace",
      "unindent": true,
      "theme": "monokai"
    },
    "insert-logo": {
      "url": "https://developers.sunbreak.io/svg/sunbreak-logo-light.svg",
      "style": "background: #2a2e31;"
    }
  }
}
