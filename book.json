{
  "title": "Sunbreak Personalization API",
  "description": "Sunbreak Personalization API",
  "gitbook": "3.x.x",
  "structure": {
    "readme": "Sunbreak.md",
    "glossary": "GLOSSARY.md"
  },
  "variables": {
    "apiUrl": "https://api.sunbreak.dev/"
  },
  "plugins": [
    "theme-api",
    "collapsible-menu",
    "hide-published-with",
    "include-codeblock",
    "ace",
    "term",
    "copy-code-button"
  ],
  "pluginsConfig": {
    "theme-api": {
      "theme": "dark",
      "split": true
    },
    "include-codeblock": {
      "template": "ace",
      "unindent": true,
      "theme": "monokai"
    }
  }
}
